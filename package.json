{
  "name": "cert-manager",
  "version": "0.2.9",
  "repository": "git@github.com:TryGhost/cert-manager.git",
  "author": "Ghost Foundation",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "node --inspect=0.0.0.0 app.js",
    "start": "node app.js",
    "test": "NODE_ENV=testing mocha './test/unit/*.test.js'",
    "test:integration": "NODE_ENV=testing mocha './test/integration/*.test.js'",
    "lint": "eslint . --ext .js --cache",
    "posttest": "yarn lint",
    "preship": "yarn test",
    "serviceBroker": "./node_modules/.bin/moleculer connect stan://localhost:4222 --ns cert-manager --id local",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
  },
  "devDependencies": {
    "eslint": "7.25.0",
    "eslint-plugin-ghost": "2.0.0",
    "mocha": "8.3.2",
    "moleculer-cli": "0.7.1",
    "node-fetch": "2.6.1",
    "should": "13.2.3",
    "sinon": "10.0.0",
    "sqlite3": "5.0.2",
    "supertest": "6.1.3"
  },
  "dependencies": {
    "@sam-lord/greenlock": "4.2.0",
    "@tryghost/acme-http-01-sequelize": "0.2.2",
    "@tryghost/fastly-js": "0.2.6",
    "@tryghost/greenlock-manager-sequelize": "0.3.0",
    "@tryghost/greenlock-store-sequelize": "0.2.0",
    "express": "5.0.0-alpha.8",
    "ghost-ignition": "4.6.1",
    "moleculer": "0.14.13",
    "mysql2": "2.2.5",
    "node-nats-streaming": "0.3.2",
    "rate-limiter-flexible": "2.2.1",
    "sequelize": "6.6.2"
  }
}
